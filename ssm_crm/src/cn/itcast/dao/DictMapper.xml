<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.itcast.dao.DictMapper">

    <select id="findDictByCode" parameterType="string" resultType="cn.itcast.pojo.BaseDict">
        SELECT * FROM base_dict a WHERE a.dict_enable=1 AND a.dict_type_code=#{code} ORDER BY a.dict_sort
    </select>

</mapper>